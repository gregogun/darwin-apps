    "@permaweb/stampjs": "0.0.15",
    "adm-zip": "0.5.10",
    "arweave": "1.11.8",
    "chalk": "4.1.2",
    "commander": "10.0.1",
    "enquirer": "2.3.6",
    "figlet": "1.6.0",
    "mime-types": "2.1.35",
    "ora": "5.4.0",
    "test-asset-sdk": "0.0.31",
